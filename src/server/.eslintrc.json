{
    "root": true,
    "parser": "babel-eslint",
    "extends": ["eslint:recommended", "google"],
    "plugins": ["googleappsscript"],
    "env": {
        "googleappsscript/googleappsscript": true
    },
    "globals": {
        "DocumentApp": true,
        "HtmlService": true,
        "global": true
    },
    "rules": {
        "import/prefer-default-export": 0
    }
}
